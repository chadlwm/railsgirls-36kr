<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>

<table>
  <tr>
    <td>ID</td>
    <td>Name</td>
    <td>Password</td>
    <td>Introduction</td>
  </tr>
  <% @friends.each do |friend| %>
    <tr>
      <td><%= friend.id %></td>
      <td><%= friend.name %></td>
      <td><%= friend.password %></td>
      <td><%= friend.introduction %></td>
    </tr>
  <% end %>
</table>
